@article{Webb2009,
abstract = {This paper describes the spatio-temporal trends in terrorist incidents in the United States, from 1970 through 2004. Utilizing the Global Terrorism Database (GTD) and ancillary data, we examine both the frequency of incidents and their character- istics: location, target type, attack type, weapon type, and perpetrator group. While the frequency of terrorist incidents has declined since the 1970s, there still is signifi- cant activity nationwide. Instead of urban-rural or West Coast-East Coast divisions, the pattern is a more complex mosaic based on group identity, target, and weapon type. We conclude that there is an explicit geography of terrorism, one that is quite decentralized and highly localized.},
annote = {Weaknesses: Only US
Stengths: Uses of GTD+Geospatial and Temporal Analysis

        
They also use ancillary data, NYT and other news sources, to contextualize the GTD.  },
author = {Webb, Jennifer J. and Cutter, Susan L.},
file = {:C$\backslash$:/Users/Lokus/Dropbox/Master Thesis/Lukas Mendeley's Safe/Webb, Cutter - The Geography of U.S. Terrorist Incidents, 1970–2004 - Terrorism and Political Violence - 2009.pdf:pdf},
