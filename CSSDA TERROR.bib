journal = {Terrorism and Political Violence},
keywords = {GTD,Geography},
mendeley-tags = {GTD,Geography},
month = jun,
number = {3},
pages = {428--449},
title = {{The Geography of U.S. Terrorist Incidents, 1970–2004}},
volume = {21},
year = {2009}
}
